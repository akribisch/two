package com.web.entity;

import java.util.Date;

/**
 * 转账记录表实体类
 * @author Administrator
 *
 */
public class Transfer {
	/**
	 * 转账表编号
	 */
	private int transferId;
	/**
	 * 转入的储蓄卡
	 */
	private Savings savingsInto;
	/**
	 * 转出的储蓄卡
	 */
	private Savings savingsOut;
	/**
	 * 转账金额
	 */
	private double money;
	/**
	 * 办理员工
	 */
	private Staff staff;
	/**
	 * 转账时间
	 */
	private Date currentTime;
	/**
	 * 转账状态
	 */
	private TransferState transferState;
	
	public Transfer() {
		super();
	}
	
	public Transfer(int transferId, Savings savingsInto, Savings savingsOut, double money, Staff staff,
			Date currentTime, TransferState transferState) {
		super();
		this.transferId = transferId;
		this.savingsInto = savingsInto;
		this.savingsOut = savingsOut;
		this.money = money;
		this.staff = staff;
		this.currentTime = currentTime;
		this.transferState = transferState;
	}
	/**
	 * @return the transferId
	 */
	public int getTransferId() {
		return transferId;
	}
	/**
	 * @param transferId the transferId to set
	 */
	public void setTransferId(int transferId) {
		this.transferId = transferId;
	}
	/**
	 * @return the savingsInto
	 */
	public Savings getSavingsInto() {
		return savingsInto;
	}
	/**
	 * @param savingsInto the savingsInto to set
	 */
	public void setSavingsInto(Savings savingsInto) {
		this.savingsInto = savingsInto;
	}
	/**
	 * @return the savingsOut
	 */
	public Savings getSavingsOut() {
		return savingsOut;
	}
	/**
	 * @param savingsOut the savingsOut to set
	 */
	public void setSavingsOut(Savings savingsOut) {
		this.savingsOut = savingsOut;
	}
	/**
	 * @return the money
	 */
	public double getMoney() {
		return money;
	}
	/**
	 * @param money the money to set
	 */
	public void setMoney(double money) {
		this.money = money;
	}
	/**
	 * @return the staff
	 */
	public Staff getStaff() {
		return staff;
	}
	/**
	 * @param staff the staff to set
	 */
	public void setStaff(Staff staff) {
		this.staff = staff;
	}
	/**
	 * @return the currentTime
	 */
	public Date getCurrentTime() {
		return currentTime;
	}
	/**
	 * @param currentTime the currentTime to set
	 */
	public void setCurrentTime(Date currentTime) {
		this.currentTime = currentTime;
	}
	/**
	 * @return the transferState
	 */
	public TransferState getTransferState() {
		return transferState;
	}
	/**
	 * @param transferState the transferState to set
	 */
	public void setTransferState(TransferState transferState) {
		this.transferState = transferState;
	}
	
}
