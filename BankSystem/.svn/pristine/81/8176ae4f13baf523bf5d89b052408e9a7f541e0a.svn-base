package com.web.entity;

import java.util.List;

public class BankMenu {

	/**
	 * 银行权限列表
	 */
	private int menuId;
	
	/**
	 * 银行权限名称
	 */
	private String menuName;
	
	/**
	 * 菜单层级
	 */
	private int menuLevel;
	
	/**
	 * 是否有此权限
	 */
	private int hasAuthority;
	
	/**
	 * 父级菜单
	 */
	private BankMenu parent;
	
	/**
	 * 父级菜单编号
	 */
	private Integer parentId;
	
	/**
	 * url地址
	 */
	private String menuUrl;
	
	private List<BankMenu> childMenu;

	public int getMenuId() {
		return menuId;
	}

	public void setMenuId(int menuId) {
		this.menuId = menuId;
	}

	public String getMenuName() {
		return menuName;
	}

	public void setMenuName(String menuName) {
		this.menuName = menuName;
	}

	public int getMenuLevel() {
		return menuLevel;
	}

	public void setMenuLevel(int menuLevel) {
		this.menuLevel = menuLevel;
	}

	public int getHasAuthority() {
		return hasAuthority;
	}

	public void setHasAuthority(int hasAuthority) {
		this.hasAuthority = hasAuthority;
	}

	public BankMenu getParent() {
		return parent;
	}

	public void setParent(BankMenu parent) {
		this.parent = parent;
	}

	public Integer getParentId() {
		return parentId;
	}

	public void setParentId(Integer parentId) {
		this.parentId = parentId;
	}

	public List<BankMenu> getChildMenu() {
		return childMenu;
	}

	public void setChildMenu(List<BankMenu> childMenu) {
		this.childMenu = childMenu;
	}

	public String getMenuUrl() {
		return menuUrl;
	}

	public void setMenuUrl(String menuUrl) {
		this.menuUrl = menuUrl;
	}
	
	
}
