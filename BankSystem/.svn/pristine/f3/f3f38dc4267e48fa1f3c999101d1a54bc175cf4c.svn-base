package com.web.entity;

/**
 * 岗位表实体类
 * @author Administrator
 *
 */
public class Station {
	/**
	 * 岗位编号
	 */
	private int statId;
	/**
	 * 岗位名称
	 */
	private String statName;
	/**
	 * 岗位人数
	 */
	private int statNum;
	/**
	 * 所属部门
	 */
	private Department department;
	
	public Station() {
		super();
	}
	
	/**
	 * @return the department
	 */
	public Department getDepartment() {
		return department;
	}

	/**
	 * @param department the department to set
	 */
	public void setDepartment(Department department) {
		this.department = department;
	}

	public Station(int statId, String statName, int statNum,Department department) {
		super();
		this.statId = statId;
		this.statName = statName;
		this.statNum = statNum;
		this.department = department;
	}

	/**
	 * @return the statId
	 */
	public int getStatId() {
		return statId;
	}

	/**
	 * @param statId the statId to set
	 */
	public void setStatId(int statId) {
		this.statId = statId;
	}

	/**
	 * @return the statName
	 */
	public String getStatName() {
		return statName;
	}

	/**
	 * @param statName the statName to set
	 */
	public void setStatName(String statName) {
		this.statName = statName;
	}

	/**
	 * @return the statNum
	 */
	public int getStatNum() {
		return statNum;
	}

	/**
	 * @param statNum the statNum to set
	 */
	public void setStatNum(int statNum) {
		this.statNum = statNum;
	}
	
}
