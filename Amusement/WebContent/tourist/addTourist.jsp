<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>添加游客</title>
</head>
<body>
	<div>
	<form action="addTourist.do" method="post">
		<div>
			游客账户:<input type="text" name="tourCount">
		</div>
		<div>
			游客名称:<input type="text" name="tourName">
		</div>
		<div>
			<input type="radio" value="0" name="tourSex">男
			<input type="radio" value="1" name="tourSex">女
		</div>
		<div>
			游客电话:<input type="text" name="tourPhone">
		</div>
		<div>
			游客电子邮箱:<input type="text" name="tourEmail">
		</div>
		<button>添加</button>
		</form>
	</div>
			
</body>
</html>